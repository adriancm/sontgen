{{#each inputs}}
<label for="{{this.id}}">{{ this.label }}</label>
<input name="text" id="{{this.id}}" placeholder="{{this.label}}" value="" data-theme="a">
{{/each}}
<fieldset class="ui-grid-a ui-responsive">
    <div class="ui-block-a">
        <input type="button" data-theme="b" data-icon="delete" data-mini="true"
               onclick="$('\#{{div}}').popup('close');" value="Cancel"/>
    </div>
    <div class="ui-block-b">
        <input type="submit" data-theme="b" data-icon="check" data-mini="true"
               onclick="editElem({
                namespace:$('#namespace').val(),
                iri:$('#iri').val()
                });" value="Edit"/>
    </div>
</fieldset>
